package formatTest;

public class FormatTask02 {
	public static void main(String[] args) {
		int kor = 0, math = 0, eng = 0;
		String name = null;
		
		name = "강대훈";
		kor = 100;
		math = 100;
		eng = 100;
		
		System.out.printf("이름: %s\n", name);
		System.out.printf("국어: %8d점\n", kor);
		System.out.printf("수학: %8d점\n", math);
		System.out.printf("영어: %8d점\n", eng);
		
	}
}
