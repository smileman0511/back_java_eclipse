package castingTask;

public class CastingTask02 {
   public static void main(String[] args) {
//      "8.43" + 2.59를 더해서 10이 나오도록 출력하기
	   String str = "";
	   double num = 0;
	   str = "8.43";
	   num = 2.59;
	   
	   int strToInt = 0; 
	   strToInt = Integer.parseInt(str); 	   
			   
	   System.out.println(strToInt);
	   
   }
}