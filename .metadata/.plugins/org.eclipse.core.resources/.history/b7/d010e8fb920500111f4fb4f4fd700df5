package castingTask;

public class CastingTask02 {
   public static void main(String[] args) {
//      "8.43" + 2.59를 더해서 10이 나오도록 출력하기
	   String str = "";
	   double num = 0;
	   str = "8.43";
	   num = 2.59;
	   
	   double strToDouble = 0; 
	   strToDouble = Double.parseDouble("8.43");
	   
	   int result1 = (int)strToDouble + (int)num;
	   String result2 = "(int)%f + (int)%f = %d";
			   
	   System.out.println(result1);
	   System.out.println(result2, strToDouble, num, strToDouble + num);
   }
}